{
    "name":  "ai-bot",
    "version":  "1.1.0",
    "private":  true,
    "license":  "ISC",
    "type":  "module",
    "description":  "",
    "main":  "dist/bootstrap.js",
    "scripts":  {
                    "start":  "node ./dist/bootstrap.js",
                    "build":  "tsc",
                    "dev:watch":  "tsc -w \u0026 node --watch dist/bootstrap.js",
                    "lint":  "eslint .",
                    "lint:fix":  "eslint . --fix",
                    "typecheck":  "tsc --noEmit",
                    "diag:quick":  "node ./dist/diagnostics/quick.js",
                    "diag:full":  "node ./dist/diagnostics/full.js",
                    "cmd":  "npm run build \u0026\u0026 node ./dist/index.js",
                    "handoff:view":  "type .\\\\.handoff\\\\context.json",
                    "handoff":  "node .\\\\scripts\\\\context-handoff.cjs",
                    "cli":  "node ./dist/cli.js",
                    "smoke":  "powershell -NoProfile -ExecutionPolicy Bypass -File .\\\\scripts\\\\smoke.ps1",
                    "handoff:refresh":  "npm run handoff \u0026\u0026 npm run handoff:view",
                    "handoff:print":  "node -e \"const fs=require(\\\"fs\\\");const j=JSON.parse(fs.readFileSync(\\\".handoff/context.json\\\",\\\"utf8\\\"));console.log({cwd:j.paths.cwd,APP_NAME:j.env.APP_NAME,NODE_ENV:j.env.NODE_ENV,package:(j.packageJson?(j.packageJson.name+\\\"@\\\"+j.packageJson.version):null)});\"",
                    "handoff:ci":  "npm run build \u0026\u0026 npm run typecheck \u0026\u0026 npm run lint \u0026\u0026 npm run diag:quick \u0026\u0026 npm run handoff:refresh",
                    "test":  "npm run build \u0026\u0026 node --test --import=tsx test/**/*.spec.ts",
                    "test:watch":  "node --test --watch --import=tsx test/**/*.spec.ts",
                    "demo":  "node ./dist/cli.js bot:send \"echo hello world\" \u0026\u0026 node ./dist/cli.js bot:send \"reverse tacos\" \u0026\u0026 node ./dist/cli.js bot:send \"ask Why Is The Sky Blue?\"",
                    "demo:q":  "set LOG_LEVEL=fatal\u0026\u0026 node ./dist/cli.js bot:send \"echo hello world\" \u0026\u0026 node ./dist/cli.js bot:send \"reverse tacos\" \u0026\u0026 node ./dist/cli.js bot:send \"ask Why Is The Sky Blue?\""
                },
    "dependencies":  {
                         "dotenv":  "^17.2.1",
                         "pino":  "^9.9.0",
                         "zod":  "^4.0.17"
                     },
    "devDependencies":  {
                            "@eslint/js":  "^9.33.0",
                            "@types/node":  "^24.3.0",
                            "@typescript-eslint/eslint-plugin":  "^8.40.0",
                            "@typescript-eslint/parser":  "^8.40.0",
                            "eslint":  "^9.33.0",
                            "eslint-config-prettier":  "^10.1.8",
                            "eslint-plugin-import":  "^2.32.0",
                            "eslint-plugin-unused-imports":  "^4.2.0",
                            "pino-pretty":  "^13.1.1",
                            "prettier":  "^3.6.2",
                            "ts-node":  "^10.9.2",
                            "tsx":  "^4.20.4",
                            "typescript":  "^5.9.2",
                            "typescript-eslint":  "^8.40.0"
                        }
}
